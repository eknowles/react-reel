{
  "name": "react-reel",
  "version": "1.2.2",
  "description": "Animate anything like a slot machine",
  "author": "Edward Knowles <mail@eknowles.com>",
  "license": "MIT",
  "repository": "eknowles/react-reel",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "scripts": {
    "test": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "prepare": "npm run build",
    "predeploy": "npm build && cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build",
    "docs:dev": "docz dev",
    "docs:build": "docz build",
    "example": "cd example/build",
    "lint": "eslint -c .eslintrc src/index.js src/theme.js --fix"
  },
  "dependencies": {
    "react-themeable": "^1.1.0"
  },
  "peerDependencies": {
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/helpers": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^8.2.3",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.15",
    "cross-env": "^5.1.4",
    "docz": "^0.10.3",
    "docz-plugin-css": "^0.10.3",
    "eslint": "^5.4.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-standard-react": "^7.0.2",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-standard": "^4.0.0",
    "gh-pages": "^1.2.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-scripts": "^1.1.5",
    "rollup": "^0.65.0",
    "rollup-plugin-babel": "^4.0.3-0",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^1.6.2",
    "rollup-plugin-url": "^1.4.0"
  },
  "files": [
    "dist"
  ]
